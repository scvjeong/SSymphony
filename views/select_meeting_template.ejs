<div class="row-fluid">
	<div class="span12">
		<div class="inner-spacer meeting-template"> 
		<!-- content goes here -->
			<a class="btn btn-success btn-large custom" title="" href="javascript:show_setting_agenda();">Custom</a>
		</div>
	</div>
</div>
<div class="row-fluid">
	<div class="span6">
		<div class="inner-spacer meeting-template"> 
		<!-- content goes here -->
			<% for(var i=0; i<(result.length/2); i++) {%>
				<div><a class="btn btn-primary btn-large" title="<%= result[i].subject %>" href="javascript:show_setting_agenda();"><%= result[i].subject %></a></div>
			<% } %>
		</div>
	</div>
	<div class="span6">
		<div class="inner-spacer meeting-template"> 
		<!-- content goes here -->
			<% for(var i=Math.ceil(result.length/2); i<result.length; i++) {%>
				<div><a class="btn btn-primary btn-large" title="<%= result[i].subject %>" href="javascript:show_setting_agenda();"><%= result[i].subject %></a></div>
			<% } %>
		</div>
	</div>
</div>
