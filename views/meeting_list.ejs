						<div>
							<div class="inner-spacer"> 
							<!-- content -->	
								<div class="accordion" id="accordion1">
									<% for(var i=0; i<result.meeting.length; i++) {%>
										<div class="accordion-group">
											<div class="accordion-heading">
												<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion1" href="#collapse<%= (i+1) %>">
													<i class="icon-plus-sign"></i> <%= result.meeting[i].subject %> </a>
											</div>
											<div id="collapse<%= (i+1) %>" class="accordion-body collapse" style="height: 0px; ">
												<div class="accordion-inner">
													회의 주제 - <%= result.meeting[i].subject %><br>
													회의 날짜 - <%= result.meeting[i].date %><br>
													회의 시간 - <%= result.meeting[i].start_time %> ~ <%= result.meeting[i].end_time %><br>
													회의 참가자 - <%= result.meeting[i].user_list %><br>
													<!--회의 역할 - 진행자: 정용기, 서기: 임종혁<br>-->
												</div>
											</div>
										</div>
									<% } %>	
								</div>
